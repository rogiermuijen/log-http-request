{
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": {
         "19": "img/icon19.png",
         "38": "img/icon38.png"
      },
      "default_popup": "popup.html",
      "default_title": "HTTP Domain Filter"
   },
   "description": "Disallow some HTTP requests.",
   "icons": {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "48": "img/icon48.png"
   },
   "manifest_version": 2,
   "name": "dFilter",
   "offline_enabled": true,
   "permissions": [ "webRequest", "webRequestBlocking", "tabs", "http://*/", "https://*/", "\u003Call_urls>", "notifications", "experimental" ],
   "version": "0.1.0"
}
